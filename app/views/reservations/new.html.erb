<%= form_with(model: @reservation, local: true) do |form| %>
  <%= form.label :room_id %>
  <%= form.collection_select :room_id, Room.all, :id, :name %>

  <%= form.label :start_date %>
  <%= form.date_select :start_date %>

  <%= form.label :end_date %>
  <%= form.date_select :end_date %>

  <%= form.submit "Create Reservation" %>
<% end %>
